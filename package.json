{
  "name": "ska-webapps-react-skeleton",
  "version": "0.0.7",
  "private": true,
  "license": "BSD 3-Clause",
  "description": "SKA React Web App Skeleton",
  "main": "index.js",
  "repository": "git@gitlab.com:ska-telescope/templates/ska-react-webapp-skeleton.git",
  "scripts": {
    "build": "webpack --mode production",
    "code-analysis": "eslint src/**/*.{js,jsx,ts,tsx} --ignore-path .eslintignore -f junit -o linting.xml",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:run:component": "cypress run --component --config video=false",
    "dev": "webpack serve --mode development --open",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "merge-reports": "jrm build/reports/unit-tests.xml \"build/tests/*.xml\"",
    "prettier": "prettier --check \"**/*\"",
    "prettier:fix": "prettier --write \"**/*\"",
    "skao:update": "yarn add @ska-telescope/ska-gui-components@latest",
    "start": "NODE_ENV=test webpack serve --mode development --open",
    "test:component:headless": "cypress run --component --headless --browser chrome --config video=false",
    "test:coverage:report": "npx nyc report --report-dir=build/reports --reporter=html",
    "test:coverage:report:ci": "npx nyc report --report-dir=build/reports --reporter=cobertura",
    "test:coverage:summary": "npx nyc report --reporter=text-summary",
    "test:cypress:component:ci": "cypress run --component --headless --browser chrome --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml --config video=false",
    "test:headless": "yarn cypress:run"
  },
  "dependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.12.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@ska-telescope/ska-gui-components": "^0.1.29",
    "bundle-loader": "^0.5.6",
    "i18next": "^21.8.9",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "moment": "^2.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.15.3",
    "react-redux": "^8.0.5",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^16.7.0",
    "redux": "^4.2.1",
    "redux-dynamic-middlewares": "^2.2.0",
    "redux-localstorage-simple": "^2.5.1",
    "redux-saga": "^1.2.3",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/runtime": "^7.17.9",
    "@types/react": "^18.0.33",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "cypress": "^12.4.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "html-webpack-plugin": "^5.5.0",
    "junit": "^1.4.9",
    "prettier": "^1.15.3",
    "sass": "^1.49.4",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
